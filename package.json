{
  "name": "nectar",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "NEXT_TELEMETRY_DISABLED=1 DIST_DIR=./dist/ next build && tsc --project tsconfig.server.json --outDir ./dist/",
    "start": "NODE_ENV=production DIST_DIR=./dist/ node dist/server/index.js",
    "analyze": "NEXT_TELEMETRY_DISABLED=1 DIST_DIR=./dev/ ANALYZE=true next build",
    "test": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@headlessui/react": "^1.1.1",
    "@heroicons/react": "^1.0.1",
    "@testing-library/react-hooks": "^7.0.2",
    "axios": "^0.21.1",
    "axios-logger": "^2.5.0",
    "axios-mock-adapter": "^1.20.0",
    "clsx": "^1.1.1",
    "cookie-session": "^1.4.0",
    "date-fns": "^2.23.0",
    "downshift": "^6.1.7",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "formik": "^2.2.9",
    "heroicon": "^0.0.2",
    "isomorphic-dompurify": "^0.15.0",
    "match-sorter": "^6.3.1",
    "neverthrow": "^4.2.1",
    "next": "^11.1.1",
    "nprogress": "^0.2.0",
    "qs": "^6.10.1",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-outside-click-handler": "^1.3.0",
    "react-query": "^3.29.0",
    "react-range": "^1.8.11",
    "react-toastify": "^8.0.2",
    "transducist": "^2.2.0",
    "uuid": "^8.3.2",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@next/bundle-analyzer": "^10.0.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/components": "^6.3.0",
    "@storybook/core-events": "^6.3.0",
    "@storybook/react": "^6.3.7",
    "@storybook/theming": "^6.3.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.6",
    "@types/cookie-session": "^2.0.42",
    "@types/dompurify": "^2.2.3",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "^26.0.23",
    "@types/katex": "^0.11.0",
    "@types/node": "^14.17.17",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.27.38",
    "@types/react": "^17.0.2",
    "@types/react-is": "^17.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.0.0",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "cypress": "^6.8.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express-validator": "^6.11.1",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.7",
    "pino-pretty": "^5.1.2",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.10",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "sharp": "^0.29.0",
    "storybook": "^6.2.9",
    "storybook-addon-next-router": "^3.0.8",
    "tailwindcss": "^2.0.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5",
    "webpack": "^5.0.0"
  },
  "volta": {
    "node": "14.16.1",
    "yarn": "1.22.15"
  },
  "jest": {
    "rootDir": "src",
    "moduleNameMapper": {
      "^@components(.*)": "<rootDir>/components$1",
      "^@api(.*)": "<rootDir>/api$1",
      "^@providers(.*)": "<rootDir>/providers$1",
      "^@(store|types|utils|hooks)": "<rootDir>/$1",
      "^@machines(.*)": "<rootDir>/machines$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/components/__mocks__/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "verbose": true
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier"
    ],
    "plugins": [
      "react",
      "react-hooks",
      "jsx-a11y",
      "@typescript-eslint",
      "jest"
    ],
    "parser": "@typescript-eslint/parser",
    "env": {
      "es6": true
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "parserOptions": {
          "project": [
            "./tsconfig.json"
          ]
        }
      }
    ],
    "rules": {
      "curly": [
        "warn",
        "all"
      ],
      "react-hooks/rules-of-hooks": "error",
      "@typescript-eslint/no-empty-function": [
        "warn"
      ],
      "@typescript-eslint/no-floating-promises": [
        "warn",
        {
          "ignoreIIFE": true
        }
      ],
      "@typescript-eslint/no-empty-interface": [
        "error",
        {
          "allowSingleExtends": false
        }
      ],
      "@typescript-eslint/unbound-method": [
        "off",
        {
          "ignoreStatic": true
        }
      ],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "ignoreRestSiblings": true
        }
      ]
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
